{
  "name": "trademind-ai",
  "version": "1.0.0",
  "description": "AI-Powered Trading Intelligence Web Application",
  "main": "index.html",
  "scripts": {
    "start": "node server.js",
    "dev": "live-server --port=3000 --open=login.html --watch=.",
    "server": "node server.js",
    "live": "live-server --port=3000 --open=login.html",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/styles.min.css styles.css login-styles.css",
    "minify-js": "uglifyjs script.js login-script.js -o dist/app.min.js",
    "serve": "http-server -p 3000 -o login.html",
    "lint": "eslint *.js",
    "format": "prettier --write *.html *.css *.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "trading",
    "ai",
    "finance",
    "web-app",
    "dashboard",
    "signals"
  ],
  "author": "TradeMind AI Team",
  "license": "MIT",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.57.0",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "prettier": "^3.2.5",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "firebase": "^10.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/trademind-ai.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/trademind-ai/issues"
  },
  "homepage": "https://github.com/your-username/trademind-ai#readme"
}
